{
	"name" : "yiisoft/yii2-app-advanced",
	"description" : "Yii 2 Advanced Project Template",
	"keywords" : [
		"yii2",
		"framework",
		"advanced",
		"project template"
	],
	"homepage" : "http://www.yiiframework.com/",
	"type" : "project",
	"license" : "BSD-3-Clause",
	"support" : {
		"issues" : "https://github.com/yiisoft/yii2/issues?state=open",
		"forum" : "http://www.yiiframework.com/forum/",
		"wiki" : "http://www.yiiframework.com/wiki/",
		"irc" : "irc://irc.freenode.net/yii",
		"source" : "https://github.com/yiisoft/yii2"
	},
	"minimum-stability" : "stable",
	"require" : {
		"php" : ">=5.6.0",
		"yiisoft/yii2" : "~2.0.6",
		"yiisoft/yii2-bootstrap" : "~2.0.0",
		"yiisoft/yii2-swiftmailer" : "~2.0.0",
		"yiisoft/yii2-imagine" : "^2.1",
		"yiisoft/yii2-httpclient" : "^2.0",
		"yiisoft/yii2-authclient" : "^2.1",
		"yiisoft/yii2-jui" : "*",
		"yiisoft/yii2-redis" : "~2.0.0",
		"zhuravljov/yii2-rest" : "*",
		"aliyuncs/oss-sdk-php" : "^2.2",
		"phpoffice/phpexcel" : "~1.8.0",
		"bower-asset/blueimp-file-upload" : "~9.11",
		"bower-asset/blueimp-gallery" : "~2.17",
		"overtrue/pinyin" : "~3.0",
		"yiisoft/yii2-queue" : "^2.0",
		"yiisoft/yii2-elasticsearch": "^2.0",
		"mongosoft/yii2-soap-client": "*",
		"zircote/swagger-php": "2.0.8",
		"mpdf/mpdf":"^8.0",
		"wechatpay/wechatpay": "^1.0",
		"wechatpay/wechatpay-guzzle-middleware": "^0.2.2"
	},
	"require-dev" : {
		"yiisoft/yii2-debug" : "~2.0.0",
		"yiisoft/yii2-gii" : "~2.0.0",
		"yiisoft/yii2-faker" : "~2.0.0",
		"codeception/base" : "^2.2.3",
		"codeception/verify" : "~0.3.1"
	},
	"config" : {
		"process-timeout": 180000,
		"fxp-asset.installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		},
		"github-oauth": {

		}
	},
	"extra" : {
		
	},
	"scripts" : {
		"post-install-cmd" : "php init --env=Development --overwrite=n"
	},
	"repositories": {
		"packagist": {
			"type": "composer",
			"url": "https://mirrors.aliyun.com/composer/"
		}
	}
}
